@model LiqunManagement.ViewModels.FormViewModels

@{
    ViewBag.Title = "檔案上傳區";
    ViewBag.Controller = "Form";
    ViewBag.Branches = "Uploads";
    Layout = "~/Views/Shared/smartadmin_Layout.cshtml";

}
<link href="~/Content/smartadmin-package-4.5.1/smartadmin-html-full/dist/css/formplugins/dropzone/dropzone.css" rel="stylesheet" />

<main id="js-page-content" role="main" class="page-content">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">
            <a href="javascript:void(0);">後臺管理系統</a>
        </li>
        <li class="breadcrumb-item">檔案上傳區</li>
        <li class="breadcrumb-item active">附件上傳</li>
    </ol>
    <div class="row">
        <div class="panel-container show col-12">
            <div class="panel-content">
                <div class="panel-tag w-100">
                    <a name="frontIdentityCardLandlord">正面身分證(房東)</a>
                </div>
                <div class="col-12">
                    <div class="row">
                        <div class="col-sm-4">
                            @using (Html.BeginForm("Uploads", "Form", FormMethod.Post, new { @class = "dropzone needsclick dz-clickable", @style = "min-height: 10rem;", @id = "dropzone1" }))
                            {
                                <input type="text" name="FormID" value="@Model.FormID" hidden />
                                <input type="text" name="Category" value="房東正面身分證" hidden />
                                <input type="text" name="Path" value="Paperwork" hidden />
                                <div class="dz-message needsclick">
                                    <br>
                                    <br>
                                    <i class="fal fa-cloud-upload text-muted mb-3"></i> <br>
                                    <span class="text-uppercase">請上傳房東正面身分證</span>
                                    <br>
                                    <span class="fs-sm text-muted">請注意檔案大小限制為 <strong>30MB</strong></span>
                                    <br>
                                    <br>
                                </div>
                            }
                        </div>
                        <div class="col-sm-8">
                            <div class="h-100" id="filenameSpace">
                                @if (Model.Uploads.frontIdentityCardLandlord != null)
                                {
                                    var i = 0;
                                    if (Model.Uploads.frontIdentityCardLandlord.Count() > 0)
                                    {
                                        <table class="table table-hover text-nowrap table-bordered mb-0">
                                            <thead>
                                                <tr style="text-align:center; vertical-align:middle; background-color:#99ccff;">
                                                    <th style=" width:60px;">編號</th>
                                                    <th>檔案名稱</th>
                                                    <th style="text-align:center; vertical-align:middle; width:60px;">操作</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                @foreach (var item in Model.Uploads.frontIdentityCardLandlord)
                                                {
                                                    i++;
                                                    <tr>
                                                        <td style="text-align:center; vertical-align:middle;">@i</td>
                                                        <td style="text-align:left; vertical-align:middle;">
                                                            @item.FileName &nbsp;&nbsp;<button class="btn btn-light btn-sm" type="button" onclick="downloadfile('@item.FileAlias','PaperWork')"><i class="fa fa-download"></i></button>
                                                        </td>
                                                        <td style="text-align:center; vertical-align:middle;">
                                                            <button type="button" class="btn btn-danger waves-effect waves-themed" onclick="deletefile('@item.FileAlias', 'frontIdentityCardLandlord')">刪除</button>
                                                        </td>
                                                    </tr>
                                                }

                                            </tbody>
                                        </table>
                                    }
                                }
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
