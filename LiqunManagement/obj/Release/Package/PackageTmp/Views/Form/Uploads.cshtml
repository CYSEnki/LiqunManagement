@section scripts
{
    <script src="~/Content/smartadmin-package-4.5.1/smartadmin-html-full/dist/js/formplugins/dropzone/dropzone.js"></script>
    <script>

        //下載檔案
        function downloadfile(filealias, path) {
            $('#hidfilealias').val(filealias);
            $('#hidpath').val(path);

            $('#HiddenDownloadFile').submit();
        };

        //刪除檔案
        function deletefile(filealias, anchor) {
            if (confirm('確認刪除檔案?')) {
                        $.ajax({
                            url: '@Url.Action("DeleteFile", "Form")',                        // url位置
                            type: 'post',                   // post/get
                            dataType: 'json',
                            data: {
                                FileAlias: filealias,
                                FileType: 'else',
                            },       // 輸入的資料
                            error: function (xhr) {
                                console.log(xhr.responseText);
                            },      // 錯誤後執行的函數
                            success: function (response) {
                                if (response !== 'nodata') {
                                    // 获取当前页面的 URL
                                    var currentURL = window.location.href;
                                    // 设置要添加的锚点名称
                                    var anchorName = anchor;
                                    // 创建包含锚点的新 URL
                                    var newURL = currentURL + '#' + anchorName;
                                    // 通过将新 URL 分配给 window.location 实现重新加载页面
                                    //window.location = newURL;
                                    window.location.reload();
                                }
                                else {
                                    alert("無操作資料，請稍後重新嘗試。")
                                }
                            }// 成功後要執行的函數
                        });
            }
        };



    </script>
}
@*Hidden form for Downlod(Download.onclick)*@
@using (Html.BeginForm("Download", "Form", FormMethod.Post, new { @id = "HiddenDownloadFile" }))
{
    @Html.Hidden("FileName", "-1", new { @id = "hidfilename" })
    @Html.Hidden("FileAlias", "-1", new { @id = "hidfilealias" })
    @Html.Hidden("Path", "-1", new { @id = "hidpath" })
}
