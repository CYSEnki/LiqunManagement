
@{
    ViewBag.Title = "表單四期";
    Layout = "~/Views/Shared/smartadmin_Layout.cshtml";
}
<style>
    .padding-top-ten {
        padding-top: 10px !important;
    }

    .panel .panel-toolbar {
        display: none;
    }

    .panel.panel-locked:not(.panel-fullscreen) .panel-hdr h2:before {
        display: none;
    }

    .form-label {
        font-weight: 600 !important;
    }
</style>
<link href="~/Content/select2/css/select2.min.css" rel="stylesheet" />

<main id="js-page-content" role="main" class="page-content">
    <ol class="breadcrumb page-breadcrumb">
        <li class="breadcrumb-item">
            <a href="javascript:void(0);">後臺管理系統</a>
        </li>
        <li class="breadcrumb-item">填寫表單</li>
        <li class="breadcrumb-item">四期</li>
        <li class="breadcrumb-item active">物件資料</li>
        <li class="position-absolute pos-top pos-right d-none d-sm-block">
            <span class="js-get-date"></span>
        </li>
    </ol>
    <div class="subheader">
        <h1 class="subheader-title">
            <i class="subheader-icon fal fa-edit"></i>
            <span class="font-weight-bold" style="padding-top:5px;">物件資料</span>
            <small>

            </small>
        </h1>
    </div>
    <div class="row">
        <div class="col-xl-12">
            <div id="panel-1" class="panel panel-locked">
                <div class="panel-hdr">
                    <h2>
                        物件資料
                    </h2>
                </div>
                <div class="panel-container show">
                    <div class="panel-content">
                        <form class="needs-validation was-validated" novalidate>
                            <div class="form-group">
                                <label class="form-label">
                                    是否為公證:
                                    <span class="text-danger">*</span>
                                </label>
                                <div class="frame-wrap padding-top-ten">
                                    <div class="custom-control custom-radio custom-control-inline custom-radio-rounded">
                                        <input type="radio" class="custom-control-input" id="Inline1Radio1_1" name="Inline1Radio1" required="">
                                        <label class="custom-control-label" for="Inline1Radio1_1">是</label>
                                    </div>
                                    <div class="custom-control custom-radio custom-control-inline custom-radio-rounded">
                                        <input type="radio" class="custom-control-input" id="Inline1Radio1_2" name="Inline1Radio1" required="">
                                        <label class="custom-control-label" for="Inline1Radio1_2">否</label>
                                        <div class="invalid-feedback">
                                            請選擇是否為公證
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="form-label" for="example-date">簽約日</label>
                                <span class="text-danger">*</span>
                                <input class="form-control" id="example-date" type="date" name="date" required>
                                <div class="invalid-feedback">
                                    請選擇簽約日
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="form-label">
                                    是否簽估價師/特色：
                                </label>
                                <span class="text-danger">*</span>
                                <div class="frame-wrap">
                                    <div class="custom-control custom-radio custom-control-inline custom-radio-rounded">
                                        <input type="radio" class="custom-control-input" id="Inline1Radio2_1" name="Inline1Radio2" required>
                                        <label class="custom-control-label" for="Inline1Radio2_1">是</label>
                                    </div>
                                    <div class="custom-control custom-radio custom-control-inline custom-radio-rounded">
                                        <input type="radio" class="custom-control-input" id="Inline1Radio2_2" name="Inline1Radio2" required>
                                        <label class="custom-control-label" for="Inline1Radio2_2">否</label>
                                    </div>
                                    <div class="invalid-feedback">
                                        請選擇是否簽估價師
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="form-label" for="simpleinput">特色</label>
                                <input type="text" id="simpleinput" class="form-control">
                            </div>
                            @*物件地址*@
                            <div class="form-group">
                                <label class="form-label">物件地址</label>
                                <span class="text-danger">*</span>
                                <div class="row">
                                    <div class="col-lg-4 pt-3">
                                        <select class="custom-select form-control select2_city" required>
                                            <option value="" selected=""></option>
                                        </select>
                                        <div class="invalid-feedback">
                                            請選擇縣/市
                                        </div>
                                    </div>
                                    <div class="col-lg-4 pt-3">
                                        <select class="custom-select form-control select2_district" required>
                                            <option value="" selected=""></option>
                                        </select>
                                        <div class="invalid-feedback">
                                            請選擇鄉/鎮/市/區
                                        </div>
                                    </div>
                                    <div class="col-lg-4 pt-3">
                                        <select class="custom-select form-control select2_road" required>
                                            <option value="" selected=""></option>
                                        </select>
                                        <div class="invalid-feedback">
                                            請選擇街/路
                                        </div>
                                    </div>
                                </div>
                            </div>
                            @*主要用途*@
                            <div class="form-group">
                                <label class="form-label">
                                    主要用途
                                </label>
                                <span class="text-danger">*</span>
                                <div class="row">
                                    <div class="col-12">
                                        <div class="frame-wrap padding-top-ten pl-2">
                                            <div class="custom-control custom-radio custom-radio-rounded">
                                                <input type="radio" class="custom-control-input" id="Inline1Radio3_1" name="Inline1Radio3" required>
                                                <label class="custom-control-label" for="Inline1Radio3_1">住家用</label>
                                            </div>
                                            <div class="custom-control custom-radio custom-radio-rounded">
                                                <input type="radio" class="custom-control-input" id="Inline1Radio3_2" name="Inline1Radio3" required>
                                                <label class="custom-control-label" for="Inline1Radio3_2">商業用</label>
                                            </div>
                                            <div class="custom-control custom-radio custom-radio-rounded">
                                                <input type="radio" class="custom-control-input" id="Inline1Radio3_3" name="Inline1Radio3" required>
                                                <label class="custom-control-label" for="Inline1Radio3_3">辦公室</label>
                                            </div>
                                            <div class="custom-control custom-radio custom-radio-rounded">
                                                <input type="radio" class="custom-control-input" id="Inline1Radio3_4" name="Inline1Radio3" required>
                                                <label class="custom-control-label" for="Inline1Radio3_4">一般事務所</label>
                                            </div>
                                            <div class="custom-control custom-radio custom-radio-rounded">
                                                <input type="radio" class="custom-control-input" id="Inline1Radio3_5" name="Inline1Radio3" required>
                                                <label class="custom-control-label" for="Inline1Radio3_5">其他:</label>
                                            </div>
                                            <input type="text" id="elsemainuse" class="form-control mt-2" placeholder="請填寫其他選項" disabled>
                                            <div class="invalid-feedback">
                                                請選擇主要用途
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            @*上傳稅單*@
                            <div class="form-group">
                                <label class="form-label">上傳稅單</label>
                                <div class="custom-file">
                                    <input type="file" class="custom-file-input" id="taxFile" disabled>
                                    <label id="taxFileLable" class="custom-file-label" for="customFile">選擇檔案</label>
                                    <div class="invalid-feedback">
                                        請上傳稅單
                                    </div>
                                </div>
                                <span class="help-block pl-2">*除了住家用以外，都要<code>&lt;上傳稅單&gt;</code></span>
                            </div>
                            @*租金/押金*@
                            <div class="row">
                                <div class="col-lg-6">
                                    <div class="form-group">
                                        <label class="form-label" for="basic-url">租金</label>
                                        <div class="input-group">
                                            <div class="input-group-prepend">
                                                <span class="input-group-text">$</span>
                                            </div>
                                            <input type="text" class="form-control" aria-label="Amount (to the nearest dollar)" placeholder="10,000" required>
                                            <div class="invalid-feedback">
                                                請填寫租金
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-6">
                                    <label class="form-label" for="basic-url">押金</label>
                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text">$</span>
                                        </div>
                                        <input type="text" class="form-control" aria-label="Amount (to the nearest dollar)" placeholder="20,000" required>
                                        <div class="invalid-feedback">
                                            請填寫押金
                                        </div>
                                    </div>
                                </div>
                            </div>
                            @*管理費*@
                            <div class="form-group">
                                <label class="form-label" for="basic-url">管理費</label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">$</span>
                                    </div>
                                    <input type="text" class="form-control" aria-label="Amount (to the nearest dollar)" placeholder="2,000" required>
                                    <div class="invalid-feedback">
                                        請填寫管理費
                                    </div>
                                </div>
                                <span class="help-block pl-2">*要含在租金內就寫<code>&lt;0&gt;</code>元</span>
                            </div>
                            <div class="row p-0">
                                <div class="col-md-5">
                                    <div class="form-group">
                                        <label class="form-label" for="example-date">起租日</label>
                                        <input class="form-control" id="example-date" type="date" name="date" required>
                                        <div class="invalid-feedback">
                                            請選擇起租日
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-5">
                                    <div class="form-group">
                                        <label class="form-label" for="example-date">結束日</label>
                                        <input class="form-control" id="example-date" type="date" name="date" required>
                                        <div class="invalid-feedback">
                                            請選擇結束日
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-2">
                                    <div class="form-group">
                                        <label class="form-label">繳租日</label>
                                        <select class="custom-select form-control" required>
                                            <option value="" selected="">繳租日</option>
                                            <option value="1">1</option>
                                            <option value="2">2</option>
                                            <option value="3">3</option>
                                            <option value="1">4</option>
                                            <option value="2">5</option>
                                            <option value="3">6</option>
                                        </select>
                                        <div class="invalid-feedback">
                                            請選擇繳租日
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group mt-3">
                                <label class="form-label">
                                    建物型態
                                </label>
                                <div class="row">
                                    <div class="col-12">
                                        <div class="frame-wrap padding-top-ten pl-2">
                                            <div class="custom-control custom-radio custom-radio-rounded">
                                                <input type="radio" class="custom-control-input" id="Inline1Radio4_1" name="Inline1Radio4" required>
                                                <label class="custom-control-label" for="Inline1Radio4_1">透天厝</label>
                                            </div>
                                            <div class="custom-control custom-radio custom-radio-rounded">
                                                <input type="radio" class="custom-control-input" id="Inline1Radio4_2" name="Inline1Radio4" required>
                                                <label class="custom-control-label" for="Inline1Radio4_2">公寓</label>
                                            </div>
                                            <div class="custom-control custom-radio custom-radio-rounded">
                                                <input type="radio" class="custom-control-input" id="Inline1Radio4_3" name="Inline1Radio4" required>
                                                <label class="custom-control-label" for="Inline1Radio4_3">華夏</label>
                                            </div>
                                            <div class="custom-control custom-radio custom-radio-rounded">
                                                <input type="radio" class="custom-control-input" id="Inline1Radio4_4" name="Inline1Radio4" required>
                                                <label class="custom-control-label" for="Inline1Radio4_4">電梯大樓</label>
                                                <div class="invalid-feedback">
                                                    請選擇建物型態
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group mt-3">
                                <label class="form-label">
                                    房/廳/衛
                                </label>
                                <div class="frame-wrap padding-top-ten pl-2">
                                    <div class="custom-control custom-radio custom-radio-rounded">
                                        <input type="radio" class="custom-control-input" id="Inline1Radio5_1" name="Inline1Radio5" required>
                                        <label class="custom-control-label" for="Inline1Radio5_1">整層出租</label>
                                    </div>
                                    <div class="custom-control custom-radio custom-radio-rounded">
                                        <input type="radio" class="custom-control-input" id="Inline1Radio5_2" name="Inline1Radio5" required>
                                        <label class="custom-control-label" for="Inline1Radio5_2">獨立套房</label>
                                        <div class="invalid-feedback">
                                            請選擇房型
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-10 col-lg-8 col-xl-6 ">
                                    <div class="input-group input-group-multi-transition w-100">
                                        <input type="text" class="form-control" aria-label="Person name" required>
                                        <div class="input-group-append">
                                            <span class="input-group-text">房</span>
                                        </div>
                                        <input type="text" class="form-control" aria-label="Person name" required>
                                        <div class="input-group-append">
                                            <span class="input-group-text">廳</span>
                                        </div>
                                        <input type="text" class="form-control" aria-label="Person name" required>
                                        <div class="input-group-append">
                                            <span class="input-group-text">衛</span>
                                        </div>
                                        <div class="invalid-feedback">
                                            請選擇房型
                                        </div>
                                    </div>
                                </div>
                            </div>
                            @*屋況附屬設備*@
                            <div class="form-group">
                                <label class="form-label" for="radio-group-1">屋況附屬設備</label>
                                <div class="row">
                                    <div class="col-lg-6 col-xl-4">
                                        <div class="input-group mt-2">
                                            <span class="input-group-text" style="width:80px;">
                                                天然瓦斯
                                            </span>
                                            <div class="input-group-prepend">
                                                <div class="input-group-text">
                                                    <div class="custom-control custom-radio">
                                                        <input type="radio" id="dRadio0_Y" name="dRadio0" class="custom-control-input">
                                                        <label class="custom-control-label" for="dRadio0_Y">是</label>
                                                    </div>
                                                </div>
                                                <div class="input-group-text">
                                                    <div class="custom-control custom-radio">
                                                        <input type="radio" id="dRadio0_N" name="dRadio0" class="custom-control-input">
                                                        <label class="custom-control-label" for="dRadio0_N">否</label>
                                                    </div>
                                                </div>
                                            </div>
                                            <input type="text" class="form-control" aria-label="Text input with radio button" id="radio-group-0">
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-xl-4">
                                        <div class="input-group mt-2">
                                            <span class="input-group-text" style="width:80px;">
                                                電視
                                            </span>
                                            <div class="input-group-prepend">
                                                <div class="input-group-text">
                                                    <div class="custom-control custom-radio">
                                                        <input type="radio" id="dRadio1_Y" name="dRadio1" class="custom-control-input">
                                                        <label class="custom-control-label" for="dRadio1_Y">是</label>
                                                    </div>
                                                </div>
                                                <div class="input-group-text">
                                                    <div class="custom-control custom-radio">
                                                        <input type="radio" id="dRadio1_N" name="dRadio1" class="custom-control-input">
                                                        <label class="custom-control-label" for="dRadio1_N">否</label>
                                                    </div>
                                                </div>
                                            </div>
                                            <input type="text" class="form-control" aria-label="Text input with radio button" id="radio-group-1">
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-xl-4">
                                        <div class="input-group mt-2">
                                            <span class="input-group-text" style="width:80px;">
                                                冷氣
                                            </span>
                                            <div class="input-group-prepend">
                                                <div class="input-group-text">
                                                    <div class="custom-control custom-radio">
                                                        <input type="radio" id="dRadio2_Y" name="dRadio2" class="custom-control-input">
                                                        <label class="custom-control-label" for="dRadio2_Y">是</label>
                                                    </div>
                                                </div>
                                                <div class="input-group-text">
                                                    <div class="custom-control custom-radio">
                                                        <input type="radio" id="dRadio2_N" name="dRadio2" class="custom-control-input">
                                                        <label class="custom-control-label" for="dRadio2_N">否</label>
                                                    </div>
                                                </div>
                                            </div>
                                            <input type="text" class="form-control" aria-label="Text input with radio button" id="radio-group-2">
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-xl-4">
                                        <div class="input-group mt-2">
                                            <span class="input-group-text" style="width:80px;">
                                                餐桌
                                            </span>
                                            <div class="input-group-prepend">
                                                <div class="input-group-text">
                                                    <div class="custom-control custom-radio">
                                                        <input type="radio" id="dRadio4_Y" name="dRadio4" class="custom-control-input">
                                                        <label class="custom-control-label" for="dRadio4_Y">是</label>
                                                    </div>
                                                </div>
                                                <div class="input-group-text">
                                                    <div class="custom-control custom-radio">
                                                        <input type="radio" id="dRadio4_N" name="dRadio4" class="custom-control-input">
                                                        <label class="custom-control-label" for="dRadio4_Y">否</label>
                                                    </div>
                                                </div>
                                            </div>
                                            <input type="text" class="form-control" aria-label="Text input with radio button" id="radio-group-4">
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-xl-4">
                                        <div class="input-group mt-2">
                                            <span class="input-group-text" style="width:80px;">
                                                洗衣機
                                            </span>
                                            <div class="input-group-prepend">
                                                <div class="input-group-text">
                                                    <div class="custom-control custom-radio">
                                                        <input type="radio" id="dRadio5_Y" name="dRadio5" class="custom-control-input">
                                                        <label class="custom-control-label" for="dRadio5_Y">是</label>
                                                    </div>
                                                </div>
                                                <div class="input-group-text">
                                                    <div class="custom-control custom-radio">
                                                        <input type="radio" id="dRadio5_N" name="dRadio5" class="custom-control-input">
                                                        <label class="custom-control-label" for="dRadio5_N">否</label>
                                                    </div>
                                                </div>
                                            </div>
                                            <input type="text" class="form-control" aria-label="Text input with radio button" id="radio-group-5">
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-xl-4">
                                        <div class="input-group mt-2">
                                            <span class="input-group-text" style="width:80px;">
                                                餐椅
                                            </span>
                                            <div class="input-group-prepend">
                                                <div class="input-group-text">
                                                    <div class="custom-control custom-radio">
                                                        <input type="radio" id="dRadio6_Y" name="dRadio6" class="custom-control-input">
                                                        <label class="custom-control-label" for="dRadio6_Y">是</label>
                                                    </div>
                                                </div>
                                                <div class="input-group-text">
                                                    <div class="custom-control custom-radio">
                                                        <input type="radio" id="dRadio6_N" name="dRadio6" class="custom-control-input">
                                                        <label class="custom-control-label" for="dRadio6_N">否</label>
                                                    </div>
                                                </div>
                                            </div>
                                            <input type="text" class="form-control" aria-label="Text input with radio button" id="radio-group-6">
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-xl-4">
                                        <div class="input-group mt-2">
                                            <span class="input-group-text" style="width:80px;">
                                                瓦斯爐
                                            </span>
                                            <div class="input-group-prepend">
                                                <div class="input-group-text">
                                                    <div class="custom-control custom-radio">
                                                        <input type="radio" id="dRadio7_Y" name="dRadio7" class="custom-control-input">
                                                        <label class="custom-control-label" for="dRadio7_Y">是</label>
                                                    </div>
                                                </div>
                                                <div class="input-group-text">
                                                    <div class="custom-control custom-radio">
                                                        <input type="radio" id="dRadio7_N" name="dRadio7" class="custom-control-input">
                                                        <label class="custom-control-label" for="dRadio7_N">否</label>
                                                    </div>
                                                </div>
                                            </div>
                                            <input type="text" class="form-control" aria-label="Text input with radio button" id="radio-group-7">
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-xl-4">
                                        <div class="input-group mt-2">
                                            <span class="input-group-text" style="width:80px;">
                                                沙發
                                            </span>
                                            <div class="input-group-prepend">
                                                <div class="input-group-text">
                                                    <div class="custom-control custom-radio">
                                                        <input type="radio" id="dRadio8_Y" name="dRadio8" class="custom-control-input">
                                                        <label class="custom-control-label" for="dRadio8_Y">是</label>
                                                    </div>
                                                </div>
                                                <div class="input-group-text">
                                                    <div class="custom-control custom-radio">
                                                        <input type="radio" id="dRadio8_N" name="dRadio8" class="custom-control-input">
                                                        <label class="custom-control-label" for="dRadio8_N">否</label>
                                                    </div>
                                                </div>
                                            </div>
                                            <input type="text" class="form-control" aria-label="Text input with radio button" id="radio-group-8">
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-xl-4">
                                        <div class="input-group mt-2">
                                            <span class="input-group-text" style="width:80px;">
                                                冰箱
                                            </span>
                                            <div class="input-group-prepend">
                                                <div class="input-group-text">
                                                    <div class="custom-control custom-radio">
                                                        <input type="radio" id="dRadio9_Y" name="dRadio9" class="custom-control-input">
                                                        <label class="custom-control-label" for="dRadio9_Y">是</label>
                                                    </div>
                                                </div>
                                                <div class="input-group-text">
